# 清理下界怪物 
# 预留保留名（不会被清理的）：Dinnerbone，Grumm，remained, qwq

# 僵尸猪灵
execute as @e[type=zombified_piglin,name=!Dinnerbone,name=!Grumm,name=!remained,name=!qwq] at @s run tp ~ -1000 ~

# 恶魂
execute as @e[type=ghast,name=!Dinnerbone,name=!Grumm,name=!remained,name=!qwq] at @s run tp ~ -1000 ~

# 凋灵骷髅
execute as @e[type=wither_skeleton,name=!Dinnerbone,name=!Grumm,name=!remained,name=!qwq] at @s run tp ~ -1000 ~

# 烈焰人
execute as @e[type=blaze,name=!Dinnerbone,name=!Grumm,name=!remained,name=!qwq] at @s run tp ~ -1000 ~

# 岩浆怪
execute as @e[type=magma_cube,name=!Dinnerbone,name=!Grumm,name=!remained,name=!qwq] at @s run tp ~ -1000 ~

# 猪灵
execute as @e[type=piglin,name=!Dinnerbone,name=!Grumm,name=!remained,name=!qwq] at @s run tp ~ -1000 ~

# 疣猪兽
execute as @e[type=hoglin,name=!Dinnerbone,name=!Grumm,name=!remained,name=!qwq] at @s run tp ~ -1000 ~

# 僵尸疣猪兽
execute as @e[type=zoglin,name=!Dinnerbone,name=!Grumm,name=!remained,name=!qwq] at @s run tp ~ -1000 ~